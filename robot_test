echo "Test X: (shortest distance, number of paths)"
echo "TEST 1: (2,2)"
./greedy_robot 1 3 4 4 3
echo "TEST 2: (Already there)"
./greedy_robot 2 1 1 1 1
echo "TEST 3: (NP, 0)"
./greedy_robot 1 1 1 5 1
echo "TEST 4: (2,2)"
./greedy_robot 2 1 2 3 5
echo "TEST 5: (Already there)"
./greedy_robot 0 3 5 3 5
echo "TEST 6: (NP, 0)"
./greedy_robot 2 1 1 3 5
echo "TEST 7: (along Diagonal points)"
./greedy_robot 1 1 1 3 3
echo "TEST 8: (along the x axis)"
./greedy_robot 3 1 1 1 5
echo "TEST 9: (Vertically along y axis)"
./greedy_robot 2 1 1 5 1
echo "TEST 10: (Np , 0)"
./greedy_robot 1 1 1 5 5
echo "TEST 11: (Edge case)"
./greedy_robot 0 1 1 1 1
echo "TEST 12: (move along same point)"
./greedy_robot 2 1 1 1 1


